{
  "name": "tailwind-nextjs-starter-blog",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "start": "next-remote-watch ./data",
    "dev": "next dev",
    "build": "next build && node --experimental-json-modules ./scripts/generate-sitemap.mjs",
    "serve": "next start",
    "analyze": "env ANALYZE=true next build",
    "prepare": "husky install",
    "lint": "prettier --check '**/*.{js,jsx,ts,tsx}' '!.next/**/*'",
    "lint:fix": "prettier --write '**/*.{js,jsx,ts,tsx}' '!.next/**/*'"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "0.8.0",
    "@mdx-js/loader": "2.1.3",
    "@mdx-js/react": "2.1.3",
    "@next/mdx": "12.2.5",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "autoprefixer": "^10.4.14",
    "gray-matter": "4.0.3",
    "image-size": "1.0.2",
    "next": "12.2.0",
    "next-mdx-remote": "4.1.0",
    "next-plausible": "3.2.0",
    "next-seo": "5.5.0",
    "next-themes": "0.2.0",
    "postcss": "8.4.24",
    "preact": "10.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-katex": "6.0.3",
    "remark-autolink-headings": "7.0.1",
    "remark-code-titles": "0.1.2",
    "remark-footnotes": "4.0.1",
    "remark-math": "5.1.1",
    "remark-slug": "7.0.1",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.2.5",
    "@next/eslint-plugin-next": "12.2.5",
    "@svgr/webpack": "6.3.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "file-loader": "6.2.0",
    "globby": "13.1.2",
    "husky": "8.0.1",
    "lint-staged": "^13.0.0",
    "next-compose-plugins": "2.2.1",
    "next-remote-watch": "1.0.0",
    "prettier": "^2.8.8",
    "rehype": "12.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-parse": "10.0.1",
    "remark-stringify": "10.0.2",
    "unist-util-visit": "4.1.1"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
