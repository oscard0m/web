{
  "name": "tailwind-nextjs-starter-blog",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "start": "next-remote-watch ./data",
    "dev": "next dev",
    "build": "next build && node --experimental-json-modules ./scripts/generate-sitemap.mjs",
    "serve": "next start",
    "analyze": "env ANALYZE=true next build",
    "prepare": "husky install",
    "lint": "prettier --check '**/*.{js,jsx,ts,tsx}' '!.next/**/*'",
    "eslint": "eslint .",
    "lint:fix": "prettier --write '**/*.{js,jsx,ts,tsx}' '!.next/**/*'",
    "eslint:fix": "eslint --fix .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "0.9.0",
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next/mdx": "14.2.3",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@vercel/analytics": "^1.2.2",
    "autoprefixer": "^10.4.14",
    "gray-matter": "4.0.3",
    "image-size": "1.1.1",
    "next": "14.2.3",
    "next-mdx-remote": "4.4.1",
    "next-plausible": "3.12.0",
    "next-seo": "6.5.0",
    "next-themes": "0.3.0",
    "postcss": "8.4.38",
    "preact": "10.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.5.0",
    "rehype-katex": "7.0.0",
    "remark-autolink-headings": "7.0.1",
    "remark-code-titles": "0.1.2",
    "remark-footnotes": "4.0.1",
    "remark-math": "6.0.0",
    "remark-slug": "7.0.1",
    "tailwindcss": "3.4.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.3",
    "@next/eslint-plugin-next": "14.2.3",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/addon-styling": "^1.3.4",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "8.1.0",
    "@tsconfig/next": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chromatic": "^11.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "file-loader": "6.2.0",
    "globby": "14.0.1",
    "husky": "9.0.11",
    "lint-staged": "^15.0.0",
    "next-compose-plugins": "2.2.1",
    "next-remote-watch": "2.0.0",
    "prettier": "^3.0.0",
    "rehype": "13.0.1",
    "remark-frontmatter": "5.0.0",
    "remark-parse": "11.0.0",
    "remark-stringify": "11.0.0",
    "storybook": "^8.0.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.4.5",
    "unist-util-visit": "5.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.14.0",
    "pnpm": "9.1.2"
  }
}
